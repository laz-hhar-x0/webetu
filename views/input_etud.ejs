  <script>
    document.getElementById('jsonFile').addEventListener('change', function(e) {
      const file = e.target.files[0];
      if (!file) return;
    
      const reader = new FileReader();
      reader.onload = function(event) {
        try {
          const json = JSON.parse(event.target.result);
    
          if (Array.isArray(json.modules)) {
            json.modules.forEach((module, index) => {
              const i = index + 1; // index Ÿäÿ®ÿØÿ£ ŸÖŸÜ 0ÿå ŸÜÿ≤ŸäÿØ 1 ÿ®ÿßÿ¥ ŸÜŸàÿßŸÅŸÇ td1, tp1...
              document.getElementById(`td${i}`).value = module.td;
              document.getElementById(`tp${i}`).value = module.tp;
              document.getElementById(`cours${i}`).value = module.cours;
            });
          } else {
            alert("‚ùå Format JSON ÿ∫Ÿäÿ± ÿµÿßŸÑÿ≠ !");
          }
        } catch (error) {
          alert("‚ùå ÿÆÿ∑ÿ£ ŸÅŸä ŸÇÿ±ÿßÿ°ÿ© ÿßŸÑŸÖŸÑŸÅ !");
        }
      };
    
      reader.readAsText(file);
    });
