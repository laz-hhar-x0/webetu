                       <h2> My Profile </h2>
                  </div>
              </nav>     
              
              <div class="profile-sections">
                <!-- DIV 1 -->
               <div class="profile-header">
                              <img src="/<%= student.imagePath || 'img/Logo2.png' %>" width="50" height="50" class="rounded-circle">                  
                  <div class="profile-text">
                    <h2><%= student.firstName + " " + student.lastName %></h2>
                      <p> University of <%= student.placeOfBirth %></p>

                  </div>
                </div>
  
            
                <!-- DIV 2 -->
                <div class="profile-info">
                  
                  <div class="info-row">
                    <i class="fa-solid fa-id-card icon"></i>
                    <div class="text">
                      <h2>Place of Birth</h2>
                      <p><%= student.placeOfBirth %></p>
                    </div>
                  </div>
                  <div class="info-row">
                    <i class="fa-solid fa-graduation-cap icon"></i>
                    <div class="text">
                      <h2>Date of Birth</h2>
                      <p><%= student.dateOfBirth ? new Date(student.dateOfBirth).toISOString().slice(0, 10) : 'N/A' %></p>
                    </div>
                  </div>
                  


                </div>
            
                <div class="language-switch">
                    <i class="fa-solid fa-language icon"></i>
